<div class="container">
    <h1>Jogadores Bons</h1>
    
    {{#each good}}

                    <input type="hidden" value={{this.id}} />
                    <h2>1#{{this.goodplayer1}}</h2>
                    <h2>2#{{this.goodplayer2}}</h2>
                    <h2>3#{{this.goodplayer3}}</h2>
                    <h2>4#{{this.goodplayer4}}</h2>
                    <h2>5#{{this.goodplayer5}}</h2>
                    <h2>6#{{this.goodplayer6}}</h2>
                    <h2>7#{{this.goodplayer7}}</h2>
                    <h2>8#{{this.goodplayer8}}</h2>
                    <div class="actions">
                        <a href="/players/updategoodplayer/{{this.id}}" ><i class="bi bi-pencil-square"></i></a>

                        <form action="/players/removegoodplayer" method="POST" >
                            <input type="hidden" name="id" value={{this.id}} />
                            <button class="delete" type="submit">
                                <i class="bi bi-x-lg"></i>
                            </button>
                    </form>
                    </div>
                    



    {{/each}}
</div>  

